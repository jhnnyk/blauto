<h1><%= @user.username %>'s cars</h1>
<ul>
  <% @user.cars.each do |car| %>
    <li><a href="/cars/<%= car.id %>"><%= car.year %> <%= car.car_make %> <%= car.car_model %></a>: <%= car.mileage %> miles</li>
  <% end %>
</ul>

<a class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored" href="/cars/new">
  <i class="material-icons">add</i>
</a>
